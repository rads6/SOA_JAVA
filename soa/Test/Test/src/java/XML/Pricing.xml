  xmlns CDATA #FIXED ''>

<!ELEMENT locationNameTo (#PCDATA)>
<!ATTLIST locationNameTo
  xmlns CDATA #FIXED ''>

<!ELEMENT customerID (#PCDATA)>
<!ATTLIST customerID
  xmlns CDATA #FIXED ''>

<data-set xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
